{% extends "base.html.twig" %}
{% block layout_content %}
    {% for posts in post %}
        <tr>
            <td>
                <h1>{{ posts.title }}</h1>
            </td>
            <td>
                <span>Crée le {{ posts.created|date("d/m/Y") }}</span>
            </td>
            <td>
                <div>{{ posts.body }}</div>
            </td>
            <td>
                <span>Modifié le {{ posts.updated|date("d/m/Y") }}</span>
            </td>
        </tr>
    {% endfor %}
{% endblock %}